# ==================================
# Wren AI Docker Environment
# Local Ollama (Gemma3:1b)
# Local Qdrant
# No OpenAI API
# ==================================

# UI settings
WREN_UI_PORT=3000
WREN_UI_HOST=0.0.0.0
WREN_UI_VERSION=0.31.1

# Engine settings
WREN_ENGINE_PORT=8080
WREN_ENGINE_VERSION=0.18.3


# AI Service settings
WREN_AI_SERVICE_PORT=5555
WREN_AI_SERVICE_VERSION=0.27.1
AI_SERVICE_FORWARD_PORT=5555

# LLM / Embedding settings
GENERATION_MODEL=gemma3:1b
LITELLM_API_BASE=http://host.docker.internal:11434/v1
LITELLM_MODEL=gemma3:1b
LITELLM_TIMEOUT=120

# Vector DB (Qdrant)
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=wren_vectors
QDRANT_DIM=3072

# SQL DB (Postgres for metadata)
MYSQL_USER=wren
MYSQL_PASSWORD=1376
MYSQL_DATABASE=wrenai
MYSQL_PORT=3306
MYSQL_HOST=wren-mysql

# OpenAI (disabled)
OPENAI_API_KEY=dummy_key

# Telemetry
TELEMETRY_ENABLED=false
USER_UUID=local-dev

# Misc
PLATFORM=linux/amd64
PROJECT_DIR=/app
LOGGING_LEVEL=DEBUG
HOST_PORT=3000
WREN_BOOTSTRAP_VERSION=0.1.5
WREN_PRODUCT_VERSION=0.27.0
IBIS_SERVER_VERSION=0.18.3
EXPERIMENTAL_ENGINE_RUST_VERSION=0.27.0
POSTHOG_API_KEY=
POSTHOG_HOST=
IBIS_SERVER_PORT=8000